{
  "name": "hejmdal",
  "version": "0.9.0",
  "description": "Adgangsplatform...",
  "main": "src/main.js",
  "scripts": {
    "dev": ". ./env.env && npm run serve",
    "dev:debug": ". ./env.env && npm run serve:debug",
    "dev:test": ". ./env.test && npm run serve",
    "build:css:dev": "rm -f static/css/* && postcss src/css/styles.css --dir static/css --watch --map --use postcss-import precss autoprefixer cssnano postcss-inline-svg postcss-custom-properties",
    "build:css:prod": "rm -f static/css/* && postcss src/css/styles.css --dir static/css --use postcss-import precss autoprefixer cssnano postcss-inline-svg postcss-custom-properties",
    "clean:errorShots": "rm -rf ./errorShots/*.png",
    "lint": "eslint ./src ./selenium ./static/js/borchk.js --cache",
    "lint:checkstyle": "eslint ./src ./selenium -o report.xml -f checkstyle",
    "ncu:check": "ncu",
    "serve": "nodemon --exec node --no-stdin src/main.js --ignore ./static/example",
    "serve:debug": "nodemon $NODE_DEBUG_OPTION --debug --exec node --no-stdin src/main.js --ignore ./static/example",
    "test": ". ./env.test && MOCK_STORAGE=1 MOCK_CULR=1 MOCK_SMAUG=1 mocha --recursive ./src/**/*.test.js --compilers js:babel-register --reporter mocha-jenkins-reporter",
    "test:integration": "npm run build:css:prod && . ./env.test && TEST_ENV=saucelabs wdio ./selenium/config/wdio.conf.js",
    "test:integration:jenkins": "npm run build:css:prod && . ./env.test && TEST_ENV=jenkins wdio ./selenium/config/wdio.conf.js",
    "test:integration:local": ". ./env.test && TEST_ENV=local wdio ./selenium/config/wdio.conf.js",
    "migrate:latest": "knex migrate:latest",
    "migrate:latest:dev": "source env.env && knex migrate:latest",
    "precommit": "npm run lint && npm run test && npm run test:integration:local",
    "start": "npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBCDK/hejmdal.git"
  },
  "author": "DBCDK",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DBCDK/hejmdal/issues"
  },
  "homepage": "https://github.com/DBCDK/hejmdal#readme",
  "engines": {
    "node": ">=6.7.0"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-register": "^6.18.0",
    "co-body": "^5.1.1",
    "knex": "0.12.9",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-combine-routers": "^1.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "^0.0.16",
    "koa-pug": "^3.0.0-2",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.1.1",
    "koa-session2": "^2.2.3",
    "koa-static": "^3.0.0",
    "lodash": "https://registry.npmjs.org/lodash/-/lodash-4.17.2.tgz",
    "moment": "^2.18.1",
    "objection": "^0.7.12",
    "pg": "^6.1.5",
    "soap": "^0.19.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-eslint": "^7.2.3",
    "chai": "^3.5.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "mocha": "^3.3.0",
    "mocha-jenkins-reporter": "0.3.8",
    "neo-async": "^2.1.0",
    "nodemon": "^1.11.0",
    "npm-check-updates": "^2.11.0",
    "postcss-cli": "^3.2.0",
    "postcss-custom-properties": "^5.0.2",
    "postcss-import": "^9.1.0",
    "postcss-inline-svg": "^2.3.0",
    "postcss-simple-vars": "^3.1.0",
    "precss": "^1.4.0",
    "read-file-stdin": "^0.2.1",
    "sinon": "^2.1.0",
    "wdio-junit-reporter": "^0.3.0",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-sauce-service": "^0.4.0",
    "wdio-selenium-standalone-service": "0.0.8",
    "webdriverio": "^4.7.1"
  }
}
