{
  "name": "hejmdal",
  "version": "0.10.0",
  "description": "Adgangsplatform...",
  "main": "src/main.js",
  "scripts": {
    "dev": ". ./env.env && npm run serve",
    "dev:debug": ". ./env.env && npm run serve:debug",
    "dev:test": "npm run build:dev &&. ./env.test && npm run serve",
    "build:prod": "npm run build:css:prod && npm run build:js:prod",
    "build:css:dev": "rm -f static/css/* && postcss src/css/styles.css --dir static/css --watch --map --use postcss-import precss autoprefixer cssnano postcss-inline-svg postcss-custom-properties",
    "build:css:prod": "rm -f static/css/* && postcss src/css/styles.css --dir static/css --use postcss-import precss autoprefixer cssnano postcss-inline-svg postcss-custom-properties",
    "build:js:dev": "rm -rf static/js/* && babel src/client --out-dir static/js --presets=es2015 --source-maps inline --watch",
    "build:js:prod": "rm -rf static/js/* && babel src/client --out-dir static/js --presets=es2015",
    "clean:errorShots": "rm -rf ./errorShots/*.png",
    "lint": "eslint ./src ./selenium --ignore-pattern 'bootstrap.min.js' --ignore-pattern 'jquery-3.1.1.slim.min.js'",
    "lint:checkstyle": "eslint ./src ./selenium --ignore-pattern 'bootstrap.min.js' --ignore-pattern 'jquery-3.1.1.slim.min.js' -o report.xml -f checkstyle",
    "ncu:check": "ncu",
    "serve": "nodemon --exec node --no-stdin src/main.js --ignore ./static/example",
    "serve:debug": "nodemon $NODE_DEBUG_OPTION --debug --exec node --no-stdin src/main.js --ignore ./static/example",
    "test": ". ./env.test && MOCK_STORAGE=1 MOCK_CULR=1 MOCK_SMAUG=1 mocha --recursive ./src/**/*.test.js --compilers js:babel-register --reporter mocha-jenkins-reporter",
    "test:integration": "npm run build:prod && . ./env.test && TEST_ENV=saucelabs wdio ./selenium/config/wdio.conf.js",
    "test:integration:jenkins": "npm run build:prod && . ./env.test && TEST_ENV=jenkins wdio ./selenium/config/wdio.conf.js",
    "test:integration:local": ". ./env.test && TEST_ENV=local wdio ./selenium/config/wdio.conf.js",
    "migrate:latest": "babel-node node_modules/.bin/knex migrate:latest",
    "migrate:latest:dev": "source env.env && knex migrate:latest",
    "migrate:rollback": "babel-node node_modules/.bin/knex migrate:rollback",
    "precommit": "npm run lint && npm run test && npm run test:integration:local",
    "start": "npm run serve",
    "debug": ". ./env.env && nodemon --exec node --inspect=9229 src/main.js --ignore ./static/example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBCDK/hejmdal.git"
  },
  "author": "DBCDK",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DBCDK/hejmdal/issues"
  },
  "homepage": "https://github.com/DBCDK/hejmdal#readme",
  "engines": {
    "node": ">=6.9.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.18.0",
    "body-parser": "^1.18.3",
    "co-body": "^5.2.0",
    "co-views": "^2.1.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-oauth-server": "^2.0.0",
    "express-session": "^1.15.6",
    "knex": "0.13.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-combine-routers": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "^0.0.16",
    "koa-pug": "^3.0.0-2",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.4.0",
    "koa-session2": "^1.0.0",
    "koa-static": "^3.0.0",
    "locale-compare-polyfill": "0.0.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "oauth2-server": "^3.0.1",
    "objection": "^0.7.1",
    "pg": "^6.1.0",
    "soap": "^0.19.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-eslint": "^7.0.0",
    "chai": "^4.1.2",
    "cssnano": "^3.7.7",
    "eslint": "^3.16.0",
    "mocha": "^3.1.0",
    "mocha-jenkins-reporter": "0.3.8",
    "neo-async": "^2.5.2",
    "nodemon": "^1.18.4",
    "npm-check-updates": "^2.14.2",
    "postcss-cli": "^4.0.0",
    "postcss-custom-properties": "^6.3.1",
    "postcss-import": "^10.0.0",
    "postcss-inline-svg": "^3.1.1",
    "postcss-simple-vars": "^4.0.0",
    "precss": "^1.4.0",
    "read-file-stdin": "^0.2.1",
    "sinon": "^2.1.0",
    "wdio-junit-reporter": "^0.3.0",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-sauce-service": "^0.4.10",
    "wdio-selenium-standalone-service": "0.0.8",
    "webdriverio": "^4.13.2"
  }
}
