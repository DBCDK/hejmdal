{
  "name": "hejmdal",
  "version": "0.10.0",
  "description": "Adgangsplatform...",
  "main": "src/main.js",
  "scripts": {
    "dev": ". ./env.env && npm run serve",
    "dev:debug": ". ./env.env && npm run serve:debug",
    "dev:test": "npm run build:dev & . ./env.test && npm run serve",
    "build:dev": "npm run build:css:dev & npm run build:js:dev",
    "build:prod": "npm run build:css:prod && npm run build:js:prod",
    "build:css:dev": "rm -f static/css/* && postcss src/css/styles.css --dir static/css --watch --map --use postcss-import precss autoprefixer cssnano postcss-inline-svg postcss-custom-properties",
    "build:css:prod": "postcss src/css/styles.css --dir static/css --use postcss-import precss autoprefixer cssnano postcss-inline-svg postcss-custom-properties",
    "start:dev": "webpack-dev-server",
    "build:js:prod": "webpack --mode production",
    "build:js:dev": "webpack --mode development --watch",
    "clean:errorShots": "rm -rf ./errorShots/*.png",
    "lint": "eslint ./src",
    "lint:checkstyle": "eslint ./src -o report.xml -f checkstyle",
    "ncu:check": "ncu",
    "serve": "nodemon --exec node --no-stdin src/main.js --ignore ./static/example --ignore ./e2e",
    "serve:debug": "nodemon $NODE_DEBUG_OPTION --debug --exec node --no-stdin src/main.js --ignore ./static/example",
    "test": ". ./env.test && MOCK_STORAGE=1 MOCK_CULR=1 MOCK_SMAUG=1 jest",
    "test:watch": "npm run test -- --watch",
    "test:cy": "npm run start:test & wait-on http://localhost:3011 && npm run cy:run",
    "test:cy-local": "npm run dev:test & npm run cy:open",
    "cy:run": "cypress run --project ./e2e",
    "cy:open": "npx cypress@latest open --project ./e2e",
    "migrate:latest": "knex migrate:latest",
    "migrate:latest:dev": ". ./env.env && npm run migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "precommit": "npm run lint && npm run test && npm run test:integration:local",
    "start": "npm run serve",
    "start:test": ". ./env.test && npm run build:prod && node src/main.js",
    "debug:test": ". ./env.test && nodemon --exec node --inspect=9229 src/main.js --ignore ./static/example --ignore ./e2e",
    "debug": ". ./env.env && nodemon --exec node --inspect=9229 src/main.js --ignore ./static/example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBCDK/hejmdal.git"
  },
  "author": "DBCDK",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DBCDK/hejmdal/issues"
  },
  "homepage": "https://github.com/DBCDK/hejmdal#readme",
  "engines": {
    "node": ">=6.9.0"
  },
  "targets": {
    "ie": "11"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.3",
    "body-parser": "^1.19.0",
    "connect-session-knex": "^1.4.0",
    "cors": "^2.8.5",
    "express": "~4.16.4",
    "express-oauth-server": "^2.0.0",
    "express-session": "^1.16.1",
    "knex": "0.16.5",
    "locale-compare-polyfill": "0.0.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "objection": "^1.6.7",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.5.0",
    "pg": "^7.9.0",
    "pug": "^2.0.3",
    "soap": "^0.27.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-cypress": "^2.2.1",
    "jest": "^24.7.1",
    "mocha-jenkins-reporter": "0.4.1",
    "neo-async": "^2.6.0",
    "nodemon": "^1.18.11",
    "npm-check-updates": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "postcss-custom-properties": "^8.0.10",
    "postcss-import": "^12.0.1",
    "postcss-inline-svg": "^3.1.1",
    "postcss-simple-vars": "^5.0.2",
    "precss": "^4.0.0",
    "wait-on": "^3.2.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
