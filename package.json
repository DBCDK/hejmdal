{
  "name": "hejmdal",
  "version": "0.1.0",
  "description": "Adgangsplatform...",
  "main": "src/main.js",
  "scripts": {
    "dev": "LOG_LEVEL=DEBUG PRETTY_LOG=1 npm run serve",
    "build:css:dev": "postcss --map --use postcss-import --use autoprefixer --use cssnano src/**/*.css > static/main.css",
    "build:css:prod": "postcss --use postcss-import --use autoprefixer --use cssnano src/**/*.css > static/main.css",
    "lint": "eslint ./src",
    "lint:checkstyle": "eslint ./src  -o report.xml -f checkstyle",
    "ncu:check": "ncu",
    "serve": "nodemon --exec node --no-stdin src/main.js",
    "test": "LOG_LEVEL=OFF mocha --recursive ./src/**/*.test.js --compilers js:babel-register --reporter mocha-jenkins-reporter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBCDK/hejmdal.git"
  },
  "author": "DBCDK",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DBCDK/hejmdal/issues"
  },
  "homepage": "https://github.com/DBCDK/hejmdal#readme",
  "engines": {
    "node": ">=6.7.0"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-register": "^6.16.0",
    "knex": "^0.12.3",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-combine-routers": "^1.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "^0.0.16",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.0.0",
    "koa-session2": "^1.0.8",
    "koa-static": "^2.0.0",
    "objection": "^0.6.0",
    "pg": "^6.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-eslint": "^7.0.0",
    "chai": "^3.5.0",
    "cssnano": "^3.7.6",
    "eslint": "^3.6.1",
    "mocha": "^3.1.0",
    "mocha-jenkins-reporter": "0.2.6",
    "nodemon": "^1.10.2",
    "npm-check-updates": "^2.8.0",
    "postcss-cli": "^2.6.0",
    "postcss-import": "^8.1.2",
    "sinon": "^1.17.6"
  }
}
